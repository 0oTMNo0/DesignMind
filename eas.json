{
  "cli": {
    "version": ">= 16.6.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": { "simulator": true },
      "cache": { "disabled": true }
    },
    "debug-apk": {
      "android": { "gradleCommand": ":app:assembleDebug" },
      "cache": { "disabled": true }
    },
    "apk": {
      "android": { "buildType": "apk" },
      "env": { "EXPO_NO_DEBUG": "1" },
      "cache": { "disabled": true }
    },
    "production": {}
  },
  "submit": {
    "production": {}
  }
}